{
  "name": "lets_beer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "NEXT_DISABLE_ESLINT=1 ESLINT_IGNORE_DISABLE=1 next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "NEXT_DISABLE_ESLINT=1 ESLINT_IGNORE_DISABLE=1 npx @cloudflare/next-on-pages",
    "pages:deploy": "npx wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.802.0",
    "@types/firebase": "^2.4.32",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.9",
    "daisyui": "^4.7.2",
    "firebase": "^11.6.1",
    "framer-motion": "^12.9.4",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.18",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^4.14.1"
  }
}
